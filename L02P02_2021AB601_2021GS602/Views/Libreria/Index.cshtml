@{
    ViewData["Title"] = "Cargar Archivos";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

    <form asp-action="SubirArchivo" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="archivo">Seleccione el archivo a cargar</label>
            <input type="file" class="form-control" id="archivo" name="archivo"></input>

        </div>

        <button type="submit" class="btn btn-primary mb-2">subir archivo </button>

    </form>
</div>
